<Window x:Class="Pierwsza_apka.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Pierwsza_apka"
        mc:Ignorable="d"
        Title="Pierwsza apka" Height="300" Width="450">
    <WrapPanel VerticalAlignment="Center" HorizontalAlignment="Center">
        <Button x:Name="btnStart" Content="Start" Height="60" Width="135" Margin="0 0 18 0"
            FontFamily="Times New Roman" FontSize="26.667" FontWeight="Bold" Click="btnStart_Click"/>
        <Button x:Name="btnTime" Content="Czas" Height="60" Width="135" Margin="18 0 0 0"
            FontFamily="Times New Roman" FontSize="26.667" FontWeight="Bold" MouseEnter="btnTime_MouseEnter" MouseLeave="btnTime_MouseLeave"/>
    </WrapPanel>
</Window>